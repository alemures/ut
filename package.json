{
  "name": "utjs",
  "version": "3.0.0",
  "description": "utjs is a high performance library with some common functions like sort, concatArrays, randomString, stringToNumber, etc.",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "mocha test/*.js",
    "test:cover": "istanbul cover _mocha test/*.js",
    "jshint": "jshint index.js lib/*.js test/*.js",
    "jscs": "jscs index.js lib/*.js test/*.js",
    "jscs:fix": "jscs --fix index.js lib/*.js test/*.js",
    "check": "npm test && npm run jshint && npm run jscs",
    "doc": "jsdoc ./lib/ ./package.json ./README.md -d ./doc"
  },
  "author": "Alejandro Santiago Nieto",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "istanbul": "~0.4.5",
    "jscs": "^3.0.7",
    "jsdoc": "^3.5.5",
    "jshint": "^2.9.6",
    "mocha": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alemures/utjs"
  }
}
